package array;

public class Product {
	private int productId;
	private String productName;
	private String float price;
	private String categrory;
	public int getProductId() {
		return productId;
	}
	public void setProductId(int productId) {
		this.productId = productId;
	}
	public String getProductName() {
		return productName;
	}
	public void setProductName(String productName) {
		this.productName = productName;
	}
	public float getPrice() {
		return price;
	}
	public void setPrice(float price) {
		this.price = price;
	}
	public String getCategrory() {
		return categrory;
	}
	public void setCategrory(String categrory) {
		this.categrory = categrory;
	}
	public Product(int productId, String productName, float price, String categrory) {
		
		this.productId = productId;
		this.productName = productName;
		this.price = price;
		this.categrory = categrory;
	}
	public Product() {
		
	}
	
	

}
